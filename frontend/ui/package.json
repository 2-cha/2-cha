{
  "name": "2cha-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prettier": "prettier --config .prettierrc --write \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "prettier:file": "prettier --config .prettierrc --write",
    "prettier:diff": "prettier --config .prettierrc --list-different \"src/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "tsc": "tsc --noEmit",
    "ci-check": "npm-run-all --parallel prettier:diff lint tsc",
    "check-all": "npm-run-all --parallel prettier lint:fix tsc",
    "msw:init": "msw init public",
    "husky:install": "cd ../.. && husky install frontend/ui/.husky",
    "prepare": "(is-ci || npm-run-all msw:init husky:install)"
  },
  "dependencies": {
    "@react-spring/web": "^9.5.5",
    "@tanstack/react-query": "^4.28.0",
    "@tanstack/react-query-devtools": "^4.29.0",
    "@types/node": "18.15.6",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-modal": "^3.16.0",
    "@types/react-slick": "^0.23.10",
    "axios": "^1.3.5",
    "classnames": "^2.3.2",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "jwt-decode": "^3.1.2",
    "next": "13.2.4",
    "oauth4webapi": "^2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-button": "^0.7.2",
    "react-hook-form": "^7.43.9",
    "react-kakao-button": "^0.0.1",
    "react-kakao-maps-sdk": "^1.1.8",
    "react-modal": "^3.16.1",
    "react-slick": "^0.29.0",
    "react-spinners": "^0.13.8",
    "react-stack-cards": "^1.0.8",
    "react-tinder-card": "^1.6.2",
    "recoil": "^0.7.7",
    "recoil-nexus": "^0.5.0",
    "recoil-persist": "^4.2.0",
    "sass": "^1.60.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^9.4.1",
    "typescript": "5.0.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.1.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss}": "yarn prettier:file"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
